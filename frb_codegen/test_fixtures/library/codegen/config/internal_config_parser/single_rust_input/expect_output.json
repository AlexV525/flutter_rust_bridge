{
  "generator": {
    "api_dart": {
      "dart3": true,
      "dart_api_class_name": "Api",
      "dart_api_instance_name": "bridge",
      "dart_decl_output_path": {
        "api": "{the-working-directory}/my_dart_folder/api.dart"
      },
      "dart_enums_style": false,
      "use_bridge_in_method": true
    },
    "wire": {
      "c": {
        "c_output_path": "{the-working-directory}/frb_generated.h",
        "rust_crate_dir": "{the-working-directory}",
        "rust_output_path": {
          "common": "{the-working-directory}/src/frb_generated.rs",
          "io": "{the-working-directory}/src/frb_generated.io.rs",
          "wasm": "{the-working-directory}/src/frb_generated.web.rs"
        }
      },
      "dart": {
        "dart_impl_output_path": {
          "common": "{the-working-directory}/my_dart_folder/frb_generated.dart",
          "io": "{the-working-directory}/my_dart_folder/frb_generated.io.dart",
          "wasm": "{the-working-directory}/my_dart_folder/frb_generated.web.dart"
        },
        "dart_root": "{the-working-directory}",
        "dart_wire_class_name": "Api",
        "extra_headers": "",
        "llvm_compiler_opts": "",
        "llvm_path": [
          "/opt/homebrew/opt/llvm",
          "/usr/local/opt/llvm",
          "/usr/lib/llvm-9",
          "/usr/lib/llvm-10",
          "/usr/lib/llvm-11",
          "/usr/lib/llvm-12",
          "/usr/lib/llvm-13",
          "/usr/lib/llvm-14",
          "/usr/lib/",
          "/usr/lib64/",
          "C:/Program Files/llvm",
          "C:/msys64/mingw64"
        ],
        "use_bridge_in_method": true,
        "wasm_enabled": true
      },
      "rust": {
        "rust_crate_dir": "{the-working-directory}",
        "rust_output_path": {
          "common": "{the-working-directory}/src/frb_generated.rs",
          "io": "{the-working-directory}/src/frb_generated.io.rs",
          "wasm": "{the-working-directory}/src/frb_generated.web.rs"
        },
        "rust_wire_mod": "frb_generated",
        "wasm_enabled": true
      }
    }
  },
  "parser": {
    "rust_crate_dir": "{the-working-directory}",
    "rust_input_path_pack": {
      "rust_input_path": {
        "api": "{the-working-directory}/src/api.rs"
      }
    }
  },
  "polisher": {
    "add_mod_to_lib": true,
    "build_runner": true,
    "c_output_path": "{the-working-directory}/frb_generated.h",
    "dart_format_line_length": 80,
    "dart_root": "{the-working-directory}",
    "duplicated_c_output_path": [],
    "rust_crate_dir": "{the-working-directory}",
    "rust_output_path": {
      "common": "{the-working-directory}/src/frb_generated.rs",
      "io": "{the-working-directory}/src/frb_generated.io.rs",
      "wasm": "{the-working-directory}/src/frb_generated.web.rs"
    },
    "wasm_enabled": true
  },
  "preparer": {
    "dart_root": "{the-working-directory}",
    "deps_check": true
  }
}